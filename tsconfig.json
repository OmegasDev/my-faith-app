{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./types", "./node_modules/@types"],
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
  "types/**/*",
  "**/*.ts",
  "**/*.tsx",
  ".expo/types/**/*.ts",
  "expo-env.d.ts",
  "nativewind-env.d.ts"
]
}
